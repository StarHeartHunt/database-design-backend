generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id                Int      @id @default(autoincrement())
  name              String   @db.VarChar(255)
  price             Int
  barCode           String
  discountPrice     Float
  discountStartedAt DateTime
  discountEndedAt   DateTime
  canDiscounted     Boolean
  stockNum          Int
  stockWarnNum      Int
  stockPlanNum      Int
  canSold           Boolean
  vendorId          Int
  manufacturerId    Int
}
